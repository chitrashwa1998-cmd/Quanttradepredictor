modules = ["python-3.11"]

[nix]
channel = "stable-24_05"
packages = ["glibcLocales"]

[deployment]
deploymentTarget = "autoscale"
run = ["streamlit", "run", "app.py", "--server.port", "5000"]

[workflows]
runButton = "TribexAlpha Fixed"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Streamlit Server"

[[workflows.workflow]]
name = "Streamlit Server"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python -m streamlit run app.py --server.port 5000"
waitForPort = 5000

[[workflows.workflow]]
name = "Start Trading App"
author = 40743738
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "pkill -f streamlit || true"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "streamlit run app.py --server.port 5000 --server.address 0.0.0.0"

[[workflows.workflow]]
name = "TribexAlpha App"
author = 40743738
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "/nix/store/rbg00q7j8bflgr0vqq1557m45lzljfav-python3-wrapper/bin/python3 -m streamlit run app.py --server.port 5000 --server.address 0.0.0.0"

[[workflows.workflow]]
name = "Streamlit Direct"
author = 40743738
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "streamlit run app.py --server.port 5000 --server.address 0.0.0.0"

[[workflows.workflow]]
name = "TribexAlpha"
author = 40743738
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "/nix/store/rbg00q7j8bflgr0vqq1557m45lzljfav-python3-wrapper/bin/python3 -m pip install --user streamlit pandas numpy plotly scikit-learn xgboost catboost lightgbm replit"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "/nix/store/rbg00q7j8bflgr0vqq1557m45lzljfav-python3-wrapper/bin/python3 -m streamlit run app.py --server.port 5000 --server.address 0.0.0.0"

[[workflows.workflow]]
name = "Run Streamlit App"
author = 40743738
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python -m pip install --upgrade streamlit pandas numpy plotly scikit-learn xgboost catboost lightgbm replit"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "streamlit run app.py --server.port 5000 --server.address 0.0.0.0 --server.headless true"

[[workflows.workflow]]
name = "TribexAlpha Fixed"
author = 40743738
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python3 -m pip install --break-system-packages streamlit pandas numpy plotly scikit-learn xgboost catboost lightgbm"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python3 -m streamlit run app.py --server.port 5000 --server.address 0.0.0.0 --server.headless true"

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 5001
externalPort = 3000
